<div class="login_container">
  <div class="login_form_wrapper">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="field">
      <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "Email", class:"table_input" %>
    </div>

    <div class="field">
      <%= f.password_field :password, autocomplete: "current-password", placeholder: "Password", class:"table_input"  %>
    </div>

    <div class="actions">
      <%= f.submit "Log in", class:"form_button" %>
    </div>
  <% end %>

  <%= render "devise/shared/links" %>
  </div>
</div>
